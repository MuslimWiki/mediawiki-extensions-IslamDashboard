{
    "name": "IslamDashboard",
    "version": "0.2.2",
    "author": "Muslim.Wiki Team",
    "url": "https://www.mediawiki.org/wiki/Extension:IslamDashboard",
    "description": "A comprehensive dashboard for MediaWiki with widgets and user customization",
    "descriptionmsg": "islamdashboard-desc",
    "license-name": "GPL-2.0-or-later",
    "type": "other",
    "requires": {
        "MediaWiki": ">= 1.43.0"
    },
    "AutoloadClasses": {
        "MediaWiki\\Extension\\IslamDashboard\\Hooks": "includes/Hooks.php",
        "MediaWiki\\Extension\\IslamDashboard\\SpecialDashboard": "includes/SpecialDashboard.php"
    },
    "Hooks": {
        "BeforePageDisplay": "MediaWiki\\Extension\\IslamDashboard\\Hooks::onBeforePageDisplay",
        "LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\IslamDashboard\\Hooks::onLoadExtensionSchemaUpdates"
    },
    "MessagesDirs": {
        "IslamDashboard": ["i18n"]
    },
    "ResourceModules": {
        "ext.islamDashboard": {
            "scripts": ["resources/modules/ext.islamDashboard.js"],
            "styles": ["resources/styles/ext.islamDashboard.less"],
            "dependencies": ["mediawiki.api", "mediawiki.jqueryMsg"],
            "position": "top"
        }
    },
    "ResourceFileModulePaths": {
        "localBasePath": "",
        "remoteExtPath": "IslamDashboard"
    },
    "manifest_version": 2
}